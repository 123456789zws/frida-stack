{
  "name": "frida-module-example",
  "version": "1.0.0",
  "description": "Example Frida module written in TypeScript",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "loose": true
              }
            ]
          ],
          "plugins": [
            [
              "@babel/plugin-transform-runtime",
              {
                "corejs": 2
              }
            ]
          ]
        }
      ]
    ]
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime-corejs2": "^7.4.5",
    "@types/frida-gum": "^13.1.0",
    "@types/node": "^12.0.4",
    "babelify": "^10.0.0",
    "typescript": "^3.5.1"
  }
}
